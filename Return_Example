pragma solidity ^0.4.9;

contract ReturnExample{
    
    function ReturnExample(){}
    
    uint tempR;
    
    function test(uint a1) returns(uint x){
        tempR = a1;
        return tempR;
    }
    
    uint tempR1;
    uint tempR2;
    
    function test2(uint b1, uint  b2) returns(uint x1, uint  x2){
        tempR1 = b1;
        tempR2 = b2;
        return (tempR1, tempR2);
    
    }
    
}
